<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <gradient android:startColor="#3359D66E" android:endColor="#3339B65E" android:angle="315"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#19B864" android:endColor="#0FA15A" android:angle="315"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#22C55E" android:endColor="#0FB863" android:angle="315"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
</selector>
